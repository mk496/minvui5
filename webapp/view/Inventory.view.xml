<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.sap.build.standard.smartStore.controller.Inventory">
	<Shell id="shell">
		<App id="app">
			<pages>
			    <Page showHeader="true" title="{i18n>appTitle}" showFooter="false">
			        <content>
			            <ObjectHeader intro="" title="{i18n>invHeaderTitle}" icon="sap-icon://retail-store" number="{inventoryView>/allItemsCount}" numberUnit="Items" numberState="None" backgroundDesign="Translucent" condensed="false" visible="true">
			                <attributes/>
			                <statuses/>
			                <markers/>
			            </ObjectHeader>
			            <OverflowToolbar width="100%" height="auto" design="Auto" enabled="true" visible="true">
			                <content>
			                    <Button id="btnSave" icon="sap-icon://save" text="{i18n>btnSave}" press="onSave" iconFirst="true" width="auto" enabled="false" visible="true" iconDensityAware="false"/>
			                    <Button id="btnOrder" icon="sap-icon://add-activity-2" text="{i18n>btnOrder}" press="onOrder" type="Accept" iconFirst="true" width="auto" enabled="false" visible="true" iconDensityAware="false"/>
			                    <Button id="btnReject" icon="sap-icon://decline" text="{i18n>btnReject}" press="onReject" type="Reject" iconFirst="true" width="auto" enabled="false" visible="true" iconDensityAware="false"/>
			                     <SearchField
									id="searchField"
									tooltip="{i18n>inventorySearchTooltip}"
									search="onItemSearch">
									<layoutData>
										<OverflowToolbarLayoutData
											maxWidth="200px"
											priority="NeverOverflow"/>
									</layoutData>
								</SearchField>
			                </content>
			            </OverflowToolbar>
			            <IconTabBar expandable="false" expanded="true" applyContentPadding="false" backgroundDesign="Solid" upperCase="false" visible="true" selectedKey="Ok">
			                <items>
			                    <IconTabFilter icon="sap-icon://drop-down-list" iconColor="Neutral" text="{i18n>allItems}" count="{inventoryView>/allItemsCount}" design="Vertical" showAll="false" textDirection="Inherit" enabled="true" visible="true" iconDensityAware="false">
			                        <content>
			                            <Table id="tableAllItems" width="auto" noDataText="{i18n>noData}" mode="None" showSeparators="All" growing="false" growingThreshold="20" growingScrollToLoad="false" class="sapUiResponsiveMargin"
			                            	items="{
			                            		path:'/InventorySet',
			                            		sorter: {
													path: 'Id',
													descending: true
												}}" updateFinished="onUpdateFinished">
			                                <infoToolbar/>
			                                <headerToolbar/>
			                                <columns>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>ProductDescription}" width="auto" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>PriceDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>ShelfStockDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Center" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>LocationDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>ImageDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>StatusDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                </columns>
			                                <items>
			                                    <ColumnListItem type="Navigation" press="onItemSelect">
			                                        <cells>
			                                            <ObjectIdentifier text="{ProductDescription}" titleActive="false" visible="true"/>
			                                            <ObjectNumber number="{Price}" unit="{Currency}" emphasized="true" state="None"/>
			                                            <StepInput value="{ShelfStock}" min="0" max="999" width="230px" step="1" enabled="{enabled}" editable="{editable}" description="{Unit}" fieldWidth="50%" textAlign="Center" change="onStockChange"/>
			                                            <Text text="{Location}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                            <Image src="/webapp/localService/{Image}" height="50px" width="50px" visible="true" densityAware="false" tooltip="{i18n>imageTooltip}"/>
			                                            <ObjectStatus title="" text="{Status}" state="{StatusColor}" visible="true"/>
			                                        </cells>
			                                    </ColumnListItem>
			                                </items>
			                            </Table>
			                        </content>
			                    </IconTabFilter>
			                    <IconTabSeparator icon="" iconDensityAware="false"/>
			                    <IconTabFilter icon="sap-icon://product" iconColor="Neutral" text="{i18n>nonPerishable}" count="{inventoryView>/nonPerishableCount}" design="Vertical" showAll="false" textDirection="Inherit" enabled="true" visible="true" iconDensityAware="false">
			                        <content>
			                            <Table id="filteredTabNonPerishable" width="auto" noDataText="{i18n>noData}" mode="None" showSeparators="All" growing="false" growingThreshold="20" growingScrollToLoad="false" class="sapUiResponsiveMargin"
			                            	items="{path:'/InventorySet', templateShareable:true}" updateFinished="onUpdateFinished">
			                                <infoToolbar/>
			                                <headerToolbar/>
			                                <columns>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>ProductDescription}" width="auto" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>PriceDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>ShelfStockDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Center" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>LocationDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>ImageDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>StatusDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                </columns>
			                                <items>
			                                    <ColumnListItem type="Active">
			                                        <cells>
			                                            <ObjectIdentifier text="{ProductDescription}" titleActive="false" visible="true"/>
			                                            <ObjectNumber number="{Price}" unit="{Currency}" emphasized="true" state="None"/>
			                                            <StepInput value="{ShelfStock}" min="0" max="999" width="230px" step="1" enabled="{enabled}" editable="{editable}" description="{Unit}" fieldWidth="50%" textAlign="Center" change="onStockChange"/>
			                                            <Text text="{Location}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                            <Image src="/webapp/localService/{Image}" height="50px" width="50px" visible="true" densityAware="false" tooltip="{i18n>imageTooltip}"/>
			                                            <ObjectStatus title="" text="{Status}" state="{StatusColor}" visible="true"/>
			                                        </cells>
			                                    </ColumnListItem>
			                                </items>
			                            </Table>
			                        </content>
			                    </IconTabFilter>
			                    <IconTabSeparator icon="" iconDensityAware="false"/>
			                    <IconTabFilter icon="sap-icon://fridge" iconColor="Neutral" text="{i18n>perishable}" count="{inventoryView>/perishableCount}" design="Vertical" showAll="false" textDirection="Inherit" enabled="true" visible="true" iconDensityAware="false">
			                        <content>
			                            <Table id="filteredTabPerishable" width="auto" noDataText="{i18n>noData}" mode="None" showSeparators="All" growing="false" growingThreshold="20" growingScrollToLoad="false" class="sapUiResponsiveMargin"
			                            	items="{path:'/InventorySet', templateShareable:true}" updateFinished="onUpdateFinished">
			                                <infoToolbar/>
			                                <headerToolbar/>
			                                <columns>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>ProductDescription}" width="auto" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>PriceDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>ShelfStockDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Center" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>LocationDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>ImageDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>StatusDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                </columns>
			                                <items>
			                                    <ColumnListItem type="Active">
			                                        <cells>
			                                            <ObjectIdentifier text="{ProductDescription}" titleActive="false" visible="true"/>
			                                            <ObjectNumber number="{Price}" unit="{Currency}" emphasized="true" state="None"/>
			                                            <StepInput value="{ShelfStock}" min="0" max="999" width="230px" step="1" enabled="{enabled}" editable="{editable}" description="{Unit}" fieldWidth="50%" textAlign="Center" change="onStockChange"/>
			                                            <Text text="{Location}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                            <Image src="/webapp/localService/{Image}" height="50px" width="50px" visible="true" densityAware="false" tooltip="{i18n>imageTooltip}"/>
			                                            <ObjectStatus title="" text="{Status}" state="{StatusColor}" visible="true"/>
			                                        </cells>
			                                    </ColumnListItem>
			                                </items>
			                            </Table>
			                        </content>
			                    </IconTabFilter>
			                    <IconTabSeparator icon="" iconDensityAware="false"/>
			                    <IconTabFilter icon="sap-icon://alert" iconColor="Critical" text="{i18n>alerts}" count="{inventoryView>/alertsCount}" design="Vertical" showAll="false" textDirection="Inherit" enabled="true" visible="true" iconDensityAware="false">
			                        <content>
			                            <Table id="filteredTabAlerts" width="auto" noDataText="{i18n>noData}" mode="None" showSeparators="All" growing="false" growingThreshold="20" growingScrollToLoad="false" class="sapUiResponsiveMargin"
			                            	items="{path:'/InventorySet', templateShareable:true}" updateFinished="onUpdateFinished">
			                                <infoToolbar/>
			                                <headerToolbar/>
			                                <columns>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>ProductDescription}" width="auto" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>PriceDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>ShelfStockDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Center" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>LocationDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>ImageDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                    <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
			                                        <header>
			                                            <Text text="{i18n>StatusDescription}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                        </header>
			                                        <footer/>
			                                    </Column>
			                                </columns>
			                                <items>
			                                    <ColumnListItem type="Active">
			                                        <cells>
			                                            <ObjectIdentifier text="{ProductDescription}" titleActive="false" visible="true"/>
			                                            <ObjectNumber number="{Price}" unit="{Currency}" emphasized="true" state="None"/>
			                                            <StepInput value="{ShelfStock}" min="0" max="999" width="230px" step="1" enabled="{enabled}" editable="{editable}" description="{Unit}" fieldWidth="50%" textAlign="Center" change="onStockChange"/>
			                                            <Text text="{Location}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit" visible="true"/>
			                                            <Image src="/webapp/localService/{Image}" height="50px" width="50px" visible="true" densityAware="false" tooltip="{i18n>imageTooltip}"/>
			                                            <ObjectStatus title="" text="{Status}" state="{StatusColor}" visible="true"/>
			                                        </cells>
			                                    </ColumnListItem>
			                                </items>
			                            </Table>
			                        </content>
			                    </IconTabFilter>
			                </items>
			                <content></content>
			            </IconTabBar>
			        </content>
			        <footer/>
			        <headerContent/>
			        <subHeader/>
			        <customHeader/>
			    </Page>
			</pages>
		</App>
    </Shell>
</mvc:View>